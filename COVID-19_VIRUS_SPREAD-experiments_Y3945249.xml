
<experiments>
  <experiment name="Restricted_Movement" repetitions="10" sequentialRunOrder="false" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <exitCondition><![CDATA[count turtles with [ infected? ] = 0 or ticks >= 50]]></exitCondition>
    <metrics>
      <metric>ticks</metric>
